#idhome.servic-descriptipn.bghome.diplaynone{style: "display:block"}
  %div
    #home
      .col-xs-9
        #axis.one.picture-sample
          = image_tag "logoend.png", :id => 'homemoo', :class => 'object bache van'
        #axis.one.about-us.diplaynone{:style => "width:100%; height:100%"}   
          %div{:style => "margin:30px; padding-top:20px"}  
