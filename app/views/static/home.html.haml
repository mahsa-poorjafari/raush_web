#idhome.servic-descriptipn.bghome.diplaynone{style: "display:block"}
  %div
    #home
      .col-xs-9
        #axis.one.picture-sample
          = image_tag "logoend.png", :id => 'homemoo', :class => 'object bache van', :title => "گروه مهندسی راش", :alt => "گروه مهندسی راش"
       
      .col-xs-3.link_to_pages
        
        .link#home-link
          #homebtn1.button.bacher.vanr.object.btnw30{waittime: "300"}
