                  .clear-float
      .col-xs-3
        #hometext.text.vanr.bacher.object{waittime: "200"}
        .link
          #homebtn1.button.bacher.vanr.object.btnw30{waittime: "300"}
            .item= t('about-us')
            .icon
              = image_tag "ICON/aboutus.png",height: "40",  width: "40", :alt => "about us icon", :title => "about us icon"
            .clear-float
