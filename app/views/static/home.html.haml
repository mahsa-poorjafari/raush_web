#idhome.servic-descriptipn.bghome.diplaynone{style: "display:block"}
  %div
    #home
      .col-xs-6
        #axis.one.picture-sample
          = image_tag "logoend.png", :id => 'homemoo', :class => 'object bache van'
        #axis.one.about-us.diplaynone{:style => "width:100%; height:100%"}   
          %div{:style => "margin:30px; padding-top:20px"}  
            - if I18n.locale == :fa
              %p{:style => "direction:rtl; text-align:justify"}
                = render 'shared/about_fa'
            - else
              %p{:style => "direction:ltr; text-align:justify"}
                = render 'shared/about_en'
              
            .link
              #btn-bk-about.button.set-center.btnw10
                .item= t('back')
                .icon
                  = image_tag "back.png", height: "40" , width: "40"
                .clear-float
      .col-xs-6
        #hometext.text.vanr.bacher.object{waittime: "200"}
        .link
          #homebtn1.button.bacher.vanr.object.btnw30{waittime: "300"}
            .item= t('about-us')
            .icon
              = image_tag "ICON/aboutus.png",height: "40",  width: "40"
            .clear-float
          #homebtn2.button.bacher.vanr.object.btnw30{waittime: "350"}
            .item= t('our-team')
            .icon
              = image_tag "ICON/ourteam.png", height: "40", width: "40"
            .clear-float
          #homebtn3.button.bacher.vanr.object.btnw30{waittime: "400"}
            .item= t('Order-Online')
            .icon
              = image_tag "ICON/onlin.png",height: "40", width: "40"
            .clear-float
        .clear-float
    #order-online.diplaynone.servic-descriptipn1
      .width80.set-center
        %h2= t ('Order-Online')
        %div{:class => I18n.locale}
          = render "order_onlines/form" 
        .clear-float
      .col-xs-6
        .link
          #btn-bk-order.button.set-center.btnw10
            .item= t('back')
            .icon
              = image_tag "back.png", height: "40" , width: "40"
            .clear-float
    /end order-online
    
    #our-team.diplaynone.servic-descriptipn1
      .width80.set-center        
        = render "teams/index"
        .clear-float
      .col-xs-6
        .link
          - if is_admin?
            .btnw30= link_to 'فرد جدید', new_team_path          
          %br        
          #btn-bk-team.button.set-center.btnw10
            .item= t('back')
            .icon
              = image_tag "back.png", height: "40" , width: "40"
            .clear-float
    /end our team
    
  / end home!
/ end HOME
