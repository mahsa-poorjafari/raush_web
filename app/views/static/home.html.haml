                = image_tag "ICON/تیم کاری راش.png", height: "40", width: "40", :alt => "تیم کاری راش", :title => "تیم کاری راش"
            .clear-float
          #homebtn3.button.bacher.vanr.object.btnw30{waittime: "400"}
            = link_to new_order_online_path do
              .item{:class => I18n.locale}= t('Order-Online')
